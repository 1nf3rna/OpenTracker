<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:maps="clr-namespace:OpenTracker.ViewModels.Maps"
             mc:Ignorable="d" x:Class="OpenTracker.Views.Maps.MapConnection"
             x:DataType="maps:MapConnectionVM" x:CompileBindings="True">
  <Line x:Name="Line"
        Classes="Connector"
        StartPoint="{Binding Start}"
        EndPoint="{Binding End}"
        Stroke="{Binding Color}"
        IsVisible="{Binding Visible}" />
</UserControl>
