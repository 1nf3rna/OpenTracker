<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:OpenTracker.ViewModels.Menus;assembly=OpenTracker"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" x:Class="OpenTracker.Views.Menus.MenuHotkeyHeader">
  <DockPanel>
    <TextBlock x:Name="Hotkey" Classes="MenuHotkey" DockPanel.Dock="Right" />
    <TextBlock x:Name="Header" Classes="Menu" />
  </DockPanel>
</UserControl>
