<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="OpenTracker.Views.UIPanel">
	<DockPanel Margin="1">
    <ContentControl Content="{Binding Dropdowns}" DockPanel.Dock="{Binding ItemsDock,
						Mode=OneWay}" />
    <ContentControl Content="{Binding Items}" DockPanel.Dock="{Binding ItemsDock,
						Mode=OneWay}" />
		<ContentControl Content="{Binding Locations}" />
	</DockPanel>
</UserControl>
